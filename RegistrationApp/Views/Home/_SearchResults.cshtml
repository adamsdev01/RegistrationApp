@model List<RegistrationApp.Controllers.SearchController.SearchResult>

@if (Model != null && Model.Any())
{
    <table class="table">
        <thead>
            <tr>
                <th>Name</th>
                <th>Account Number</th>
                <!-- Add other columns as needed -->
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Name</td>
                    <td>@item.AccountNumber</td>
                    <!-- Add other columns as needed -->
                </tr>
            }
        </tbody>
    </table>
}
